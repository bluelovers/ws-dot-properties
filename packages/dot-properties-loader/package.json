{
  "name": "dot-properties-loader",
  "version": "1.0.1",
  "description": "load / save java.util.Properties.",
  "keywords": [
    "load",
    "java",
    "properties",
    "dot-properties",
    ".properties",
    "parse",
    "stringify",
    "ast",
    "comment",
    "escape",
    "chinese",
    "unicode",
    "utf-8",
    "resource",
    "bundle",
    "i18n",
    "utf8",
    "lines",
    "save"
  ],
  "homepage": "https://github.com/bluelovers/ws-dot-properties/tree/master/packages/dot-properties-loader#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/ws-dot-properties/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/ws-dot-properties.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:jest": "jest --coverage",
    "test:mocha": "npx mocha --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "npm:publish": "npm publish",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "dot-properties2": "^1.0.0",
    "ts-type": "^1.2.15"
  },
  "devDependencies": {
    "@bluelovers/fast-glob": "^3.0.4",
    "@types/bluebird": "^3.5.30",
    "@types/fs-extra": "^8.1.0",
    "array-hyper-unique": "^1.1.0",
    "fs-extra": "^9.0.0"
  }
}
